{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "outputDirectory": "public",
  "routes": [
    {
      "src": "^/(?!.*\\.(js|css|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot)$)(.*)$",
      "dest": "/api/lambda.php"
    },
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    }
  ]
}
